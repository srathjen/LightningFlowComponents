                                    <script>
                                        // Create a namespace for our custom functions
                                        YAHOO.namespace("cyangate.com");

                                        YAHOO.cyangate.com.hideCopyUrlPanel = function() {
                                            document
                                                    .getElementById("copyUrlPanel").style.display = "none";
                                            YAHOO.cyangate.com.copyUrlPanel
                                                    .hide();
                                        }

                                        YAHOO.cyangate.com.showCopyUrlPanel = function() {
                                            document
                                                    .getElementById("copyUrlPanel").style.display = "block";
                                            YAHOO.cyangate.com.copyUrlPanel
                                                    .show();
                                        }

                                        YAHOO.cyangate.com.hideDownloadAreaPanel = function() {
                                            document
                                                    .getElementById("downloadAreaPanel").style.display = "none";
                                            YAHOO.cyangate.com.downloadAreaPanel
                                                    .hide();
                                        }

                                        YAHOO.cyangate.com.showDownloadAreaPanel = function() {
                                            document
                                                    .getElementById("downloadAreaPanel").style.display = "block";
                                            YAHOO.cyangate.com.downloadAreaPanel
                                                    .show();
                                        }

                                        YAHOO.cyangate.com.hideConfirmDeletePanel = function() {
                                            document
                                                    .getElementById("confirmDeletePanel").style.display = "none";
                                            YAHOO.cyangate.com.confirmDeletePanel
                                                    .hide();
                                        }

                                        YAHOO.cyangate.com.showConfirmDeletePanel = function() {
                                            document
                                                    .getElementById("confirmDeletePanel").style.display = "block";
                                            YAHOO.cyangate.com.confirmDeletePanel
                                                    .show();
                                        }

                                        YAHOO.cyangate.com.showEmailPanel = function() {
                                            document
                                                    .getElementById("emailPanel").style.display = "block";
                                            YAHOO.cyangate.com.emailPanel
                                                    .show();
                                        }

                                        YAHOO.cyangate.com.hideEmailPanel = function() {
                                            YAHOO.cyangate.com.emailPanel
                                                    .hide();
                                            var frame = document
                                                    .getElementById("emailIframe");
                                            if (frame) {
                                                frameDoc = frame.contentDocument
                                                        || frame.contentWindow.document;
                                                frameDoc.documentElement.innerHTML = "";
                                            }
                                        }

                                        function selectText() {
                                            var inputElem = document
                                                    .getElementsByTagName("input");
                                            for (var i = 0; i < inputElem.length; i++) {
                                                if (inputElem[i].id
                                                        .indexOf("attachmentFileUrl") != -1) {

                                                    if (!(inputElem[i].createTextRange)) {
                                                        inputElem[i].focus();
                                                        inputElem[i]
                                                                .setSelectionRange(
                                                                        0, 9999);
                                                    } else {
                                                        setSelectionRange(
                                                                inputElem[i],
                                                                0, 9999)
                                                    }

                                                    return;
                                                }
                                            }
                                        }

                                        function setSelectionRange(input,
                                                selectionStart, selectionEnd) {

                                            var range = input.createTextRange();
                                            range.collapse(true);
                                            range.moveEnd('character',
                                                    selectionEnd);
                                            range.moveStart('character',
                                                    selectionStart);
                                            range.select();

                                        }

                                        YAHOO.cyangate.com.init = function() {
                                            document.body.className = document.body.className
                                                    + " yui-skin-sam";
                                            
                                            YAHOO.cyangate.com.downloadAreaPanel = new YAHOO.widget.Panel(
                                                    "downloadAreaPanel", // The id of our dialog container
                                                    {
                                                        width : '210px',
                                                        visible : false, // Should be invisible when rendered
                                                        draggable : true, // Make the dialog draggable
                                                        close : false, // Don't include a close title button
                                                        modal : true, // Make it modal
                                                        fixedCenter : true, // Keep centered if window is scrolled
                                                        zindex : 90, // Make sure it's on top of everything

                                                        // This line adds the appear/vanish fade effect
                                                        effect : {
                                                            effect : YAHOO.widget.ContainerEffect.FADE,
                                                            duration : 0.35
                                                        }
                                                    });
                                            YAHOO.cyangate.com.copyUrlPanel = new YAHOO.widget.Panel(
                                                    "copyUrlPanel", // The id of our dialog container
                                                    {
                                                        width : '160px',
                                                        visible : false, // Should be invisible when rendered
                                                        draggable : true, // Make the dialog draggable
                                                        close : false, // Don't include a close title button
                                                        modal : true, // Make it modal
                                                        fixedCenter : true, // Keep centered if window is scrolled
                                                        zindex : 90, // Make sure it's on top of everything

                                                        // This line adds the appear/vanish fade effect
                                                        effect : {
                                                            effect : YAHOO.widget.ContainerEffect.FADE,
                                                            duration : 0.35
                                                        }
                                                    });
                                            YAHOO.cyangate.com.confirmDeletePanel = new YAHOO.widget.Panel(
                                                    "confirmDeletePanel", // The id of our dialog container
                                                    {
                                                        width : '150px',
                                                        visible : false, // Should be invisible when rendered
                                                        draggable : true, // Make the dialog draggable
                                                        close : false, // Don't include a close title button
                                                        modal : true, // Make it modal
                                                        fixedCenter : true, // Keep centered if window is scrolled
                                                        zindex : 90, // Make sure it's on top of everything

                                                        // This line adds the appear/vanish fade effect
                                                        effect : {
                                                            effect : YAHOO.widget.ContainerEffect.FADE,
                                                            duration : 0.35
                                                        }
                                                    });

                                            // Render the dialog to the document.body level of the DOM

                                            YAHOO.cyangate.com.copyUrlPanel
                                                    .render(document.body);
                                            YAHOO.cyangate.com.downloadAreaPanel
                                                    .render(document.body);
                                            YAHOO.cyangate.com.confirmDeletePanel
                                                    .render(document.body);

                                        }

                                        // Add the init method to the window.load event
                                        YAHOO.util.Event
                                                .addListener(window, "load",
                                                        YAHOO.cyangate.com.init);
                                    </script>
