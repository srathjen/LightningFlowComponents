                                                    <apex:outputField value="{!currWishChild.LastName__c}"/>
                                                </div>
                                               </div>

                                              <div class="form-group row col-lg-6">
                                               <label class="col-sm-3" aria-labelledby="Child Name">DATE OF BIRTH</label>
