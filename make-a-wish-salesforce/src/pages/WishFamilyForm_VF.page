                                                        - Remove
                                                        <apex:param name="index" value="{!cnt}"/>
                                                    </apex:commandlink>
                                                    <script type="text/javascript">
                                                    listSize = {!cnt};
                                                    </script>
                                                    <apex:variable var="cnt" value="{!cnt+1}"/>
