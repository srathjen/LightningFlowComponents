                                                                <label class="sr-only" aria-label="last_name">Last Name</label>
                                                            </div>
                                                            <div class="col-sm-6">
                                                                <apex:inputText value="{!wishFamilContact.lastName}" styleClass="form-control lastname" html-placeholder="Last Name" />
                                                            </div>
                                                        </div>
