                                            <!-- Account Description -->
                                            <apex:outputPanel rendered="{!If(((isbudgetApprovalExist == true && expense.Budget_Approval_Status__c != 'Submitted') || expense.Projected_Approval_Status__c == 'Approved'),true,false)}">
