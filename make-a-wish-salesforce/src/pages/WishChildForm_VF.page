           <script>
            $(".date-input").datepicker({
                autoclose: true,
                endDate: '+0d'
            });
            $('#childForm a').click(function(){
                $('#childForm a').css('font-size','1rem');
                $(this).css('font-size','1.2rem');
            });
            function formatDate(date) {
            var d = new Date(date),
            month = '' + (d.getUTCMonth() + 1),
            day = '' + d.getUTCDate(),
            year = d.getUTCFullYear();
            
            if (month.length < 2) month = '0' + month;
            if (day.length < 2) day = '0' + day;
            
            return [month,day,year].join('/');
            }
            
             if($(".date-input").val() != ""){
               var date = formatDate($(".date-input").val());
               $(".date-input").val(date);
            }
           
          </script>
