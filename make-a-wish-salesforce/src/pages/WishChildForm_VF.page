                                                <div class="col-xs-12 col-sm-8"><apex:inputTextarea value="{!currWishChild.Other__c}" styleClass="form-control" html-placeholder="Please enter details here."/></div>
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
