    <script>
    
   $(document).ready(function(){ 
       
        $('.readonly').attr("readonly","readonly");
        $(".numeric").keyup(function(){
            var x = $(".a").val();
            if(validate(x)){
                alert('Please Enter Valid Phone Number');
            }
        });
        
        
         $('.collapse a').click(function() { 
           $('.active').removeClass('active');
           $('.'+$(this).attr('class')).addClass('active');                      
       });
        
         $(".phonecheck").click(function(){
            
            var x = $(".a").val();
            
            if(validate(x)){
                alert('Please Enter Valid Phone Number');
                return false;
             }
             
           else if(x !='')
            {
                  x= x.replace("(", "");
                  x= x.replace(")", "");
                  x= x.replace("-", "");
                  x= x.replace(" ", "");
                 if(x.length<10 || x.length>10)
                 {
                  alert('Please Enter 10 digit Phone number');
                  return false;
                 }
            }
            
              
        });
        
        function validate(a)
        {
        
            var x = a;
            x= x.replace("(", "");
            x= x.replace(")", "");
            x= x.replace("-", "");
            x= x.replace(" ", "");
        
            if(x != '')
            {
                if(!$.isNumeric(x))
                   return true;
            }
        }
        
        
        
        
        var windw = this;        
        $.fn.followTo = function ( pos ) {
            var $this = this,
            $window = $(windw);
         $window.scroll(function(e){
            if ($window.scrollTop() > pos) {
                $this.css({
                    position: 'absolute',
                    top: pos
                  });
             }
            else {
                $this.css({
                position: 'fixed',
                top: 200
               });
             }
           });
         };
        
        $('.sticky').followTo($(".clearBoth").height()-250);
                 
       
    });
    
   
    function ClickToValidate(){       
        
           var runBool = true;
           $('.required').each(function(){
               if($(this).val() == '' || $(this).val() == null ){
                   runBool = false;
                   alert("Please fill all required fields");
                   return false;
               }
                  
           });
           if(!runBool){
              return false;
              }
           else{
              var a = validateEmail();
              if(a)
                  return true;
              else
                  return false;
              }
        }
        
       function validateEmail() {
       var isBool=true; 
       $('.emailValue').each(function(){ 
       var filter = /^[\w\-\.\+]+\@[a-zA-Z0-9\.\-]+\.[a-zA-z0-9]{2,4}$/;
            if($(this).val()!= '' && $(this).val()!= null){
                if (!filter.test($(this).val())){
                  isBool = false;
                  //alert('Invalid email address, please enter a valid email address or leave as blank');                   
                    if($('.Yes').val() == "Yes" && $('.Yes').val() == null){
                        $('.Yes').css('border', '2px red solid');
                    }
                }
              }
             
        }); 

          if(!isBool ){
              $($('.errorMsg').text("Invalid email address, please enter a valid email address or leave as blank")).css('color', 'Red');           
              return false;
          }
          else{
             
              return true;
          }
       }
        
      
        
    </script>
    
