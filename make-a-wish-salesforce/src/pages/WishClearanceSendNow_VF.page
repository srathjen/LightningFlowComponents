<apex:page sidebar="false" applyBodyTag="false" cache="false" standardController="case" extensions="WishClearanceSendNow_AC">

  
  <apex:form id="clearanceForm">
  <apex:messages />
  <apex:pageBlock title="Wish Clearance">
      <apex:pageBlockSection title="Wish Clearance Form Details" columns="2">
     <apex:outputField label="Medical Professional Name" value="{!currentParentCase.Contact.Hidden_Medical_Physician__c}" />
     <apex:outputField label="Medical Professional Email" value="{!currentParentCase.Contact.Hidden_Medical_Physician_Email__c }" style="display:none" />
     <apex:InputField label="Wish Involves Air Travel" value="{!currentParentCase.wish_involves_air_travel__c}" rendered="{!currentParentCase.wish_involves_air_travel__c == Null}" />
     <apex:OutputField label="Wish Involves Air Travel" value="{!currentParentCase.wish_involves_air_travel__c}" rendered="{!currentParentCase.wish_involves_air_travel__c != Null}" />
      </apex:pageBlockSection>
  
  </apex:pageBlock>
  <center>
    <apex:commandButton value="Send Now" action="{!SendtoDocuSign}"/>
    <apex:commandButton value="Cancel" action="{!Cancel}" />
  </center>
  </apex:form>
 
</apex:page>