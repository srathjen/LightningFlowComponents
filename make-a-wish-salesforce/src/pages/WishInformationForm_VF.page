                                                    <apex:selectList value="{!wcfUtil.availableYear1}" id="availability-year-one" multiselect="false" size="1" styleClass="form-control year1 col-xs-12 col-sm-12 c-select yr">
                                                        <apex:selectOptions value="{!wcfUtil.Year}" />
                                                    </apex:selectList>
