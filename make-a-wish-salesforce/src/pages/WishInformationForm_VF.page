    <script type="text/javascript">
     function ClickToValidate(){       
        
           var runBool = true;
           $('.required').each(function(){
               if($(this).val() == '' || $(this).val() == null ){
                   runBool = false;
                   alert("Please fill all required fields");
                   if($('.Yes').val().length == 0 && $('.style1').val() == 'Yes')
                      $('.Yes').css('border', '2px red solid');
                      
                  if($('.wheelchairtext').val().length == 0 && $('.wheelchairpicklist').val() == 'Yes')                      
                      $('.wheelchairtext').css('border', '2px red solid');
                      
                  if($('.oxygennotes').val().length == 0 && $('.oxygenpicklist').val() == 'Yes')
                      $('.oxygennotes').css('border', '2px red solid');
                      
                   return false;
               }
                  
           });
           if(!runBool){
              return false;
              }
           else{
              $('.Yes').css('border', '1px #ccc solid');
              $('.wheelchairtext').css('border', '2px #ccc solid');
              $('.oxygennotes').css('border', '2px #ccc solid');
              WishInfoSaveAction();
              return true;
              }
        }
    </script>
    
