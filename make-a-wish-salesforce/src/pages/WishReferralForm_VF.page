            $('<meta name="ref-chid" content=""><meta name="ref-mawsite" content="salesforce">').insertAfter('#cleartype');
    </script>
    <script type="text/javascript">
      //script to resize iframe
      function iframe_resize() {
        var parentDomain = (parent !== window) ? document.referrer : document.location;
        parentDomain = parentDomain.split('/')[2];

        //alert(parentDomain);
        var body = document.body,
          html = document.documentElement,
          height = Math.max(body.scrollHeight, body.offsetHeight,
            html.clientHeight, html.scrollHeight, html.offsetHeight);
        if (parent.postMessage) {
          parent.postMessage(height, "*");
        }
        body.scrollTop = document.documentElement.scrollTop = 0;
      }

      //script to pull chid argument from the URL
      function getQueryVariable(variable) {
        var query = window.location.search.substring(1);
        var vars = query.split("&");
        for (var i = 0; i < vars.length; i++) {
          var pair = vars[i].split("=");
          if (pair[0] == variable) {
            return pair[1];
          }
        }
        return (false);
      }

      //populate meta tags based on chid argument
      var refchid = getQueryVariable('chid');
      $('meta[name=ref-chid]').attr('content', refchid);
    </script>
