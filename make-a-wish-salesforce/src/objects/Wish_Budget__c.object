    <fields>
        <fullName>Total_FE_Posted_In_Kind_to_Cash_Ratio__c</fullName>
        <description>Provides the percentage of In-Kind relative to the total of all In-Kind and Cash expenses.</description>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(Total_FE_Posted_Cash__c) &amp;&amp; ISBLANK(Total_FE_Posted_In_Kind__c), NULL,
IF(!ISBLANK(Total_FE_Posted_Cash__c) &amp;&amp; ISBLANK(Total_FE_Posted_In_Kind__c), 0,
IF(ISBLANK(Total_FE_Posted_Cash__c) = NULL &amp;&amp; !ISBLANK(Total_FE_Posted_In_Kind__c), 1,
IF(!ISBLANK(Total_FE_Posted_Cash__c) &amp;&amp; !ISBLANK(Total_FE_Posted_In_Kind__c), (Total_FE_Posted_In_Kind__c / (Total_FE_Posted_Cash__c + Total_FE_Posted_In_Kind__c)),NULL))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total FE Posted In-Kind to Cash Ratio</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
