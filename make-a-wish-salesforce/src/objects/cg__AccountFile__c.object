        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(!(ISBLANK(cg__Shortcut__c )),  RIGHT(cg__Shortcut__c, (LEN(cg__Shortcut__c) - FIND(&apos;:&apos;, cg__Shortcut__c))) ,IF(cg__File_Size_in_Bytes__c &gt; 1024, 
