    <validationRules>
        <fullName>Do_not_set_picklist_question_Responses</fullName>
        <active>true</active>
        <description>For Picklist questions, please specify the next action on each response option, not on the Medical Question.  Next Action on the Medical Question is for Comment question types only.</description>
        <errorConditionFormula>(!ISBLANK(TEXT(Next_Action__c)) ||  !ISBLANK(Specified_Question__c)) &amp;&amp;  ISPICKVAL(Data_Type__c, "Picklist - Single")</errorConditionFormula>
        <errorMessage>For Picklist questions, please specify the next action on each response record, not on the Medical Question record directly.  Next Action + Specified Questions on the Medical Question record are for Comment question types only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Next_Action_is_Question_select_Question</fullName>
        <active>true</active>
        <description>If the Next Action is a Specified Question, please select a Medical Question.</description>
        <errorConditionFormula>ISPICKVAL(Next_Action__c, "Specified Question") &amp;&amp;  ISBLANK(Specified_Question__c)</errorConditionFormula>
        <errorDisplayField>Specified_Question__c</errorDisplayField>
        <errorMessage>If the Next Action is a Specified Question, please select a Medical Question.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Next_Action_not_a_question_no_question</fullName>
        <active>true</active>
        <description>If the Next Action is not a specified question, please leave the specified question field blank.</description>
        <errorConditionFormula>!ISPICKVAL( Next_Action__c , "Specified Question") &amp;&amp; !ISBLANK( Specified_Question__c)</errorConditionFormula>
        <errorDisplayField>Specified_Question__c</errorDisplayField>
        <errorMessage>If the Next Action is not a specified question, please leave the specified question field blank.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
