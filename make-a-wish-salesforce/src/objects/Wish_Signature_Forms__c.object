        <label>All</label>
    </listViews>
    <listViews>
        <fullName>All1</fullName>
        <columns>NAME</columns>
        <columns>Auto_add_for_Participant__c</columns>
        <columns>Auto_add_for_Participant_Guardian__c</columns>
        <columns>Auto_add_for_Wish_Child__c</columns>
        <columns>Auto_add_for_Wish_Child_Guardian__c</columns>
        <columns>Diagnosis_Verification_for_Lead_Convert__c</columns>
        <columns>Is_Clearance__c</columns>
        <filterScope>Everything</filterScope>
        <label>All Forms with Auto-Add fields</label>
    </listViews>
    <nameField>
        <label>Wish Signature Form Name</label>
        <trackHistory>false</trackHistory>
        <type>Text</type>
    </nameField>
    <pluralLabel>Wish Signature Forms</pluralLabel>
