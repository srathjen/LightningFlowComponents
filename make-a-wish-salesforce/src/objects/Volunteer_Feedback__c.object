    <validationRules>
        <fullName>Rating_between_1_and_5</fullName>
        <active>true</active>
        <errorConditionFormula>Rating__c &lt; 1
||
 Rating__c &gt; 5</errorConditionFormula>
        <errorDisplayField>Rating__c</errorDisplayField>
        <errorMessage>Please Select a Rating between 1 and 5</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Select_either_Wish_OR_Non_Wish_Event</fullName>
        <active>true</active>
        <description>Select either Wish OR Non Wish Event.  Feedback cannot be for both at the same time.</description>
        <errorConditionFormula>NOT(ISBLANK(Non_Wish_Events__c))  
&amp;&amp;
NOT(ISBLANK(Wish__c))</errorConditionFormula>
        <errorMessage>Select either a Wish OR a Non Wish Event.  Feedback cannot be provide for both at the same time.  Please create one record per event or wish.</errorMessage>
    </validationRules>
