    <validationRules>
        <fullName>Vol_Opp_Do_Not_Create_for_Closed_Wish</fullName>
        <active>true</active>
        <description>Should not create volunteer opportunity for Closed Wish.</description>
        <errorConditionFormula>Wish__r.IsClosed=True &amp;&amp; ISNEW() &amp;&amp; $Profile.Name  != &apos;Integration&apos;</errorConditionFormula>
        <errorMessage>You cannot create volunteer opportunity for closed wish.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Volunteer_Opp_Hold_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED( Status__c ), NOT(Hidden_allow_Hold_status__c), ISPICKVAL(Status__c , &apos;Hold&apos;) )</errorConditionFormula>
        <errorMessage>Hold is a system-generated status and cannot be manually assigned.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Volunteer_Opp_Inactive_Status</fullName>
        <active>true</active>
        <description>This validation will fire when the Volunteer Opportunity status is &apos;Inactive&apos; and Inactive field is unchecked.</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c,&apos;Inactive&apos;),Inactive__c = false )</errorConditionFormula>
        <errorMessage>Please check the Inactive check box.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Volunteer_Opp_Reason_Inactive</fullName>
        <active>true</active>
        <description>This rule will fire if the if Reason Inactive is null when Inactive is checked.</description>
        <errorConditionFormula>AND(Inactive__c = true,ISBLANK(TEXT(Reason_Inactive__c )))</errorConditionFormula>
        <errorMessage>Please enter the value for Reason Inactive</errorMessage>
    </validationRules>
