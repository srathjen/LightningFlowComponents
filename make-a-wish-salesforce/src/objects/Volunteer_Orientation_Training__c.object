    <fields>
        <fullName>Orientation_Training_email_signature__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Account_Name__c =&quot;&quot; &amp;&amp; Class_Offering__r.Owner:User.Email=&quot;&quot; &amp;&amp; Class_Offering__r.Owner:User.Phone=&quot;&quot;, Class_Offering__r.Owner:User.FirstName +&apos; &apos;+ Class_Offering__r.Owner:User.LastName,

IF(Account_Name__c =&quot;&quot; &amp;&amp; Class_Offering__r.Owner:User.Phone=&quot;&quot;, Class_Offering__r.Owner:User.FirstName +&apos; &apos;+ Class_Offering__r.Owner:User.LastName + BR() + Class_Offering__r.Owner:User.Email,

IF(Account_Name__c =&quot;&quot; &amp;&amp; Class_Offering__r.Owner:User.Email=&quot;&quot;, Class_Offering__r.Owner:User.FirstName +&apos; &apos;+ Class_Offering__r.Owner:User.LastName + BR() + Class_Offering__r.Owner:User.Phone,

IF(Class_Offering__r.Owner:User.Email=&quot;&quot; &amp;&amp; Class_Offering__r.Owner:User.Phone=&quot;&quot;, Class_Offering__r.Owner:User.FirstName +&apos; &apos;+ Class_Offering__r.Owner:User.LastName + BR() + Account_Name__c ,

IF(Account_Name__c =&quot;&quot;, Class_Offering__r.Owner:User.FirstName +&apos; &apos;+ Class_Offering__r.Owner:User.LastName + BR() +  Class_Offering__r.Owner:User.Phone + BR() + Class_Offering__r.Owner:User.Email,

IF(Class_Offering__r.Owner:User.Phone=&quot;&quot;, Class_Offering__r.Owner:User.FirstName +&apos; &apos;+ Class_Offering__r.Owner:User.LastName + BR() +  Account_Name__c   + BR() + Class_Offering__r.Owner:User.Email,

IF(Class_Offering__r.Owner:User.Email=&quot;&quot;, Class_Offering__r.Owner:User.FirstName +&apos; &apos;+ Class_Offering__r.Owner:User.LastName + BR() +  Account_Name__c   + BR() + Class_Offering__r.Owner:User.Phone,


Class_Offering__r.Owner:User.FirstName +&apos; &apos;+ Class_Offering__r.Owner:User.LastName + BR() +  Account_Name__c   + BR() + Class_Offering__r.Owner:User.Phone + BR() + Class_Offering__r.Owner:User.Email
)))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Orientation Training email signature</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
