        <externalId>false</externalId>
        <label>Restrict Access</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>Wish Affiliation Security Settings</label>
    <visibility>Public</visibility>
</CustomObject>
