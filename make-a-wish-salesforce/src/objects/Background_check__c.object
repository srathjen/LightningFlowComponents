    <fields>
        <fullName>Background_Check_Email_signature__c</fullName>
        <description>This is Used in Background check email templates.</description>
        <externalId>false</externalId>
        <formula>IF( Volunteer__r.Account.Name=&quot;&quot; &amp;&amp; Owner:User.Phone=&quot;&quot; &amp;&amp; Owner:User.Email=&quot;&quot;, Owner:User.FirstName +&apos; &apos;+ Owner:User.LastName, 

IF( Volunteer__r.Account.Name=&quot;&quot; &amp;&amp; Owner:User.Phone=&quot;&quot;, Owner:User.FirstName +&apos; &apos;+ Owner:User.LastName + BR() + Owner:User.Email, 
IF( Volunteer__r.Account.Name=&quot;&quot; &amp;&amp; Owner:User.Email=&quot;&quot;, Owner:User.FirstName +&apos; &apos;+ Owner:User.LastName + BR() + Owner:User.Phone, 
IF(Owner:User.Phone=&quot;&quot; &amp;&amp; Owner:User.Email=&quot;&quot;, Owner:User.FirstName +&apos; &apos;+ Owner:User.LastName + BR() + Volunteer__r.Account.Name , 

IF( Volunteer__r.Account.Name=&quot;&quot;, Owner:User.FirstName +&apos; &apos;+ Owner:User.LastName + BR() + Owner:User.Phone + BR()+ Owner:User.Email, 
IF(Owner:User.Phone=&quot;&quot;, Owner:User.FirstName +&apos; &apos;+ Owner:User.LastName + BR() + Volunteer__r.Account.Name  + BR() + Owner:User.Email , 
IF(Owner:User.Email=&quot;&quot;, Owner:User.FirstName +&apos; &apos;+ Owner:User.LastName + BR()+ Volunteer__r.Account.Name  +BR() + Owner:User.Phone, 

Owner:User.FirstName +&apos; &apos;+ Owner:User.LastName + BR() + Volunteer__r.Account.Name + BR()+ Owner:User.Phone+ BR()+ Owner:User.Email 
)))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Background Check Email signature</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
