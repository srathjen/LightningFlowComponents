    <fields>
        <fullName>Days_From_Signature_to_Expiry__c</fullName>
        <description>Days from Signature Date to Expiry Date, if Status is Signed and Signed Date is completed. Auto-populated from the Wish Signature Form.</description>
        <externalId>false</externalId>
        <label>Days From Signature to Expiry</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Days_to_Expiry__c</fullName>
        <description>This formula shows a number countdown of days from Signature Date to Expiry Date along with an indicator icon. This only shows if Status is Signed and Signed Date has a value.
Icon made by Smashicons from www.flaticon.com and is licensed by CC 3.0 BY.</description>
        <externalId>false</externalId>
        <formula>IF((Text(Status__c)!= &quot;Completed&quot; &amp;&amp; Text(Status__c) != &quot;Signed&quot;) || ISBLANK(Signed_Date__c ),&quot;&quot;,
IF(Days_From_Signature_to_Expiry__c &lt; 0, &quot; Expired&quot;, 
IF(Days_From_Signature_to_Expiry__c &lt;= 30, TEXT( Signature_Expiry_Date__c - TODAY() ) &amp; &quot;!&quot;, TEXT( Signature_Expiry_Date__c - TODAY() )
)
)
)</formula>
        <inlineHelpText>Days until this signature expires. This only shows if Status is Signed and Signed Date has a value.</inlineHelpText>
        <label>Days to Expiry</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>DocuSign_Status__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>DocuSign Status</label>
        <referenceTo>dsfs__DocuSign_Status__c</referenceTo>
        <relationshipLabel>Wish Required Signatures</relationshipLabel>
        <relationshipName>Wish_Required_Signatures</relationshipName>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
