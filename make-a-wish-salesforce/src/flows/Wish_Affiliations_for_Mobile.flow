TRIM(RIGHT({!Wish_Case.Active_Wish_Granter__c}, (LEN({!Wish_Case.Active_Wish_Granter__c}) - FIND(&quot;,&quot;, {!Wish_Case.Active_Wish_Granter__c}))))
