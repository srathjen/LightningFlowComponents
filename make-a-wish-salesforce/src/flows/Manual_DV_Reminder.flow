    <decisions>
        <name>Has_a_DV_Been_Sent</name>
        <label>A DV Been Sent Electronically?</label>
        <locationX>249</locationX>
        <locationY>331</locationY>
        <defaultConnector>
            <targetReference>Which_Warning_Screen_to_Display</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Lead.Part_A_Sent__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Lead.Using_Paper_Process_For_DV__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Lead.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Referred</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Lead.Sub_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Pending Diagnosis Verification</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Manual_DV_Reminder</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
